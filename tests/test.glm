// Simple model with three loads for testing mysql group_recorder

//0
#set profiler=1
//1
module mysql;
//2
object database {   
   hostname "localhost";
   username "gridlabd";
   password "";
   schema "gridlabd";
   port 3306;
   tz_offset 0;
}
//3
module powerflow {
	solver_method NR;
};
//4
clock {
	timezone PST+8PDT;
	starttime '2001-01-01 00:00:00';
	stoptime '2001-01-02 00:00:00';
}
//5
// swing node
object meter {
	name substation_node;
	phases ABCN;
	nominal_voltage 7200;
	bustype SWING;
}
//6
// line configuration objects
object line_configuration {
    name overhead_line_1;
    conductor_A overhead_line_conductor_1;
    conductor_B overhead_line_conductor_1;
    conductor_C overhead_line_conductor_1;
    conductor_N overhead_line_conductor_1;
    spacing line_spacing_1;
}
//7
object line_spacing {
    name line_spacing_1;
    distance_AB 4;
    distance_BC 4;
    distance_AC 4;
    distance_AN 3;
    distance_BN 3;
    distance_CN 3;
}
//8
object overhead_line_conductor {
    name overhead_line_conductor_1;
    geometric_mean_radius 0.0271;
    resistance 0.0369;
}
//9
// Line, load, and meter 1
object overhead_line {
    name ol_1;
    phases ABCN;
    from substation_node;
    to meter_1;
    length 606.04;
    configuration overhead_line_1;
}
//10
object load {
	phases ABCN;
	name load_1;
	parent meter_1;
	base_power_A 120000;
	base_power_B 130000;
	base_power_C 110000;
	power_pf_A 0.98;
	current_pf_A 0.92;
	impedance_pf_A 0.95;
	power_fraction_A 0.8;
	current_fraction_A 0.15;
	impedance_fraction_A 0.05;
	power_pf_B 0.98;
	current_pf_B 0.92;
	impedance_pf_B 0.95;
	power_fraction_B 0.1;
	current_fraction_B 0.3;
	impedance_fraction_B 0.6;
	power_pf_C 0.98;
	current_pf_C 0.92;
	impedance_pf_C 0.95;
	power_fraction_C 0.5;
	current_fraction_C 0.1;
	impedance_fraction_C 0.4;
	nominal_voltage 7200;
}
//11
object meter {
	name meter_1;
	phases ABCN;
	nominal_voltage 7200;
	groupid meter_group;
}
//12
// Line, load, and meter 2
object overhead_line {
    name ol_2;
    phases ABCN;
    from substation_node;
    to meter_2;
    length 1606.04;
    configuration overhead_line_1;
}
//13
object load {
	phases ABCN;
	name load_2;
	parent meter_2;
	base_power_A 130000;
	base_power_B 110000;
	base_power_C 120000;
	power_pf_A 0.98;
	current_pf_A 0.92;
	impedance_pf_A 0.95;
	power_fraction_A 0.8;
	current_fraction_A 0.15;
	impedance_fraction_A 0.05;
	power_pf_B 0.98;
	current_pf_B 0.92;
	impedance_pf_B 0.95;
	power_fraction_B 0.1;
	current_fraction_B 0.3;
	impedance_fraction_B 0.6;
	power_pf_C 0.98;
	current_pf_C 0.92;
	impedance_pf_C 0.95;
	power_fraction_C 0.5;
	current_fraction_C 0.1;
	impedance_fraction_C 0.4;
	nominal_voltage 7200;
}
//14
object meter {
	name meter_2;
	phases ABCN;
	nominal_voltage 7200;
	groupid meter_group;
}
//15
// Line, load, and meter 3
object overhead_line {
    name ol_3;
    phases ABCN;
    from substation_node;
    to meter_3;
    length 66.04;
    configuration overhead_line_1;
}
//16
object load {
	phases ABCN;
	name load_3;
	parent meter_3;
	base_power_A 110000;
	base_power_B 120000;
	base_power_C 130000;
	power_pf_A 0.98;
	current_pf_A 0.92;
	impedance_pf_A 0.95;
	power_fraction_A 0.8;
	current_fraction_A 0.15;
	impedance_fraction_A 0.05;
	power_pf_B 0.98;
	current_pf_B 0.92;
	impedance_pf_B 0.95;
	power_fraction_B 0.1;
	current_fraction_B 0.3;
	impedance_fraction_B 0.6;
	power_pf_C 0.98;
	current_pf_C 0.92;
	impedance_pf_C 0.95;
	power_fraction_C 0.5;
	current_fraction_C 0.1;
	impedance_fraction_C 0.4;
	nominal_voltage 7200;
}
//17
object meter {
	name meter_3;
	phases ABCN;
	nominal_voltage 7200;
	groupid meter_group;
}
//18
// Group recorders
// energy
object recorder {
  parent meter_3;
  interval 30;
  property "voltage_A.real", "voltage_A.imag[V]", "voltage_B";
  recorder_name "Normal Recorder mode";
  file property_test_1;
  mode "w+";
  options UNITS;
  query_buffer_limit 500;
};
//19
object recorder {
  parent meter_3;
  interval 30;
  property "voltage_A", "voltage_B";
  file property_test_2;
  header_fieldnames "name";
  mode "w+";
  options UNITS;
    query_buffer_limit 500;
};
//20
object recorder {
  group "groupid=meter_group";
  interval 30;
  property "voltage_A.real","voltage_A.imag","voltage_B";
  recorder_name "Group Recorder mode";
  file property_group_test;
  mode "w+";
  options UNITS;
  query_buffer_limit 2000;
};
